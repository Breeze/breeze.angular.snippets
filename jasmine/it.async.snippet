<?xml version="1.0" encoding="utf-8"?>
<CodeSnippets xmlns="http://schemas.microsoft.com/VisualStudio/2005/CodeSnippet">
  <CodeSnippet Format="1.0.0">
    <Header>
      <Title>it async</Title>
      <Author>Ward Bell</Author>
      <Description>Jasmine it async template</Description>
      <HelpUrl>http://jasmine.github.io/2.0/introduction.html#section-Asynchronous_Support</HelpUrl>
      <SnippetTypes />
      <Keywords />
      <Shortcut>ita</Shortcut>
    </Header>
    <Snippet>
      <References />
      <Imports />
      <Declarations>
        <Literal Editable="true">
          <ID>should</ID>
          <Type />
          <ToolTip />
          <Default>should</Default>
          <Function />
        </Literal>
        <Literal Editable="true">
          <ID>code</ID>
          <Type />
          <ToolTip />
          <Default>//your code here</Default>
          <Function />
        </Literal>
      </Declarations>
      <Code Language="JavaScript" Delimiter="$"><![CDATA[it('$should$', function(done) {
        $code$$end$
});]]></Code>
    </Snippet>
  </CodeSnippet>
</CodeSnippets>